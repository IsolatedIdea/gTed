#include <stdlib.h>

#include "Definitions.h"
#include "NativeTerminal.h"

//-----------------------------------------------------------ProcessKeypress(..)
void ProcessKeypress()
{
	char c = ReadKey();

	switch(c)
	{
		case CTRL_KEY('q'):
		{
			exit(0);
		}
		break;
	}
}//ProcessKeypress(...) ends
